    <% if flash[:notice].present? %>
	    <div class="container text-center alert alert-primary">
	    	<%= flash[:notice] %>
	    </div>
    <% end %>

    <% if flash[:alert].present? %>
	    <div class="container text-center alert alert-warning">
	    	<%= flash[:alert] %>
	    </div>
    <% end %>

    <% if flash[:success].present? %>
	    <div class="container text-center alert alert-success">
	    	<%= flash[:success] %>
	    </div>
    <%end%>
    <% if flash[:error].present? %>
	    <div class="container text-center alert alert-danger">
	    	<%= flash[:error] %>
	    </div>
    <% end %>


 

