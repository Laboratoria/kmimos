<h1>Listing provider_attachments</h1>

<table>
  <thead>
    <tr>
      <th>Provider</th>
      <th>Photo</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @provider_attachments.each do |provider_attachment| %>
      <tr>
        <td><%= provider_attachment.provider_id %></td>
        <td><%= provider_attachment.photo %></td>
        <td><%= link_to 'Show', provider_attachment %></td>
        <td><%= link_to 'Edit', edit_provider_attachment_path(provider_attachment) %></td>
        <td><%= link_to 'Destroy', provider_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Provider attachment', new_provider_attachment_path %>
